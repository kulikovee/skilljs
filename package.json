{
  "name": "skilljs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "docker": "npm run docker-clean && npm run docker-build && npm run docker-up",
    "docker-up": "docker-compose up nextjs-docker-container",
    "docker-build": "docker-compose build --no-cache",
    "docker-clean": "docker-compose down -v",
    "docker-remove-containers": "docker rm -f $(docker ps -a -q)",
    "docker-remove-volumes": "docker volume rm $(docker volume ls -q)"
  },
  "dependencies": {
    "@types/node": "20.4.0",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "autoprefixer": "10.4.14",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.8",
    "next": "13.4.8",
    "postcss": "8.4.25",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.6"
  }
}
